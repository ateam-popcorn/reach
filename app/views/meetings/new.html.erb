<h1>会議を作る</h1>

<%= form_for @meeting do |f| %>
  <div class="form-group">
    <%= f.label :start_at %>
    <%= f.text_field :start_at, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :end_at %>
    <%= f.text_field :end_at, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.submit class: 'btn btn-primary' %>
  </div>
<% end %>
